alias ls="ls -l -G"
alias h="history"
alias hg="history | grep"

alias v='vim'

alias dev='cd $DEV_HOME'
alias dotfiles='cd $DOTFILES_HOME'

alias okta="gopass -c okta"
alias p="gopass -c"
alias shadd="ssh-add ~/.ssh/github_rsa ~/.ssh/gitlab_rsa"

function http_server {
  python -m SimpleHTTPServer $1 > /dev/null 2>&1
}

function proj {
  cd $DEV_HOME/projects/$1/$2
}
